{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ldapenv next dev",
    "build": "ldapenv next build",
    "start": "ldapenv next start",
    "ilb-build": "ldapenv prisma generate && ldapenv next build",
    "ilb-deploy": "ldapenv prisma migrate deploy",
    "clean": "rimraf ./.next ./out",
    "check:es:build": "npx es-check es5 './.next/static/**/*.js' -v",
    "escheck": "pnpm run clean && ldapenv pnpm run build && pnpm run check:es:build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "prisma": {
    "seed": "ldapenv node --experimental-specifier-resolution=node prisma/seed.mjs"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.18.6",
    "@ilb/node_rest": "^1.1.6",
    "@prisma/client": "^3.15.2",
    "ajv": "^8.11.0",
    "ajv-i18n": "^4.2.0",
    "antd": "^4.23.5",
    "awilix": "^7.0.3",
    "bcrypt": "^5.0.1",
    "glob": "^8.0.3",
    "next": "12.3.0",
    "next-auth": "^4.9.0",
    "next-compose-plugins": "^2.2.1",
    "next-connect": "^1.0.0-next.2",
    "next-transpile-modules": "^9.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.53.0",
    "uniforms": "^3.10.0-rc.1",
    "uniforms-antd": "^3.10.0-rc.1",
    "uniforms-bridge-json-schema": "^3.10.0-rc.1"
  },
  "devDependencies": {
    "@ilb/ldapenv-cli": "^1.0.10",
    "@next/bundle-analyzer": "^13.1.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "prisma": "^3.15.2",
    "rimraf": "^4.1.2"
  }
}
